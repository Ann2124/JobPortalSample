@model IEnumerable<JobPortalSample.Models.Application>

@{
    ViewBag.Title = "Viewapplied";
}

<h2>View Applied</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Openings.Designation)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem=>item.user.Firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Openings.Designation)
        </td>
      
        <td>
           
            @Html.ActionLink("Details", "Details", new { id=item.ApplicationId }) 
            
        </td>
    </tr>
}

</table>
